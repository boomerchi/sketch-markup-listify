{
	"name": "Markup Listify",
	"description": "Get your HTML for lists.",
	"author": "Marisa Roque",
	"authorEmail": "marisa.miranda.roque@gmail.com",
	"homepage": "https://github.com/marisaroque/sketch-markup-listify",
	"version": 1.1,
	"identifier": "sketch-markup-listify",
	"appcast": "https://raw.githubusercontent.com/marisaroque/sketch-markup-listify/master/appcast.xml",
	"compatibleVersion": 46,
	"bundleVersion": 1,
	"commands": [
		{
			"name": "Get HTML for lists",
			"identifier": "onRunMarkup",
			"shortcut": "alt command l",
			"script": "markup-listify.js",
			"handler": "onRunMarkup"
		},
		{
			"name": "Get Jade for lists",
			"identifier": "onRunJade",
			"shortcut": "ctrl alt command j",
			"script": "markup-listify.js",
			"handler": "onRunJade"
		},
		{
			"name": "Get Markdown for lists",
			"identifier": "onRunMarkdown",
			"shortcut": "ctrl alt command m",
			"script": "markup-listify.js",
			"handler": "onRunMarkdown"
		},
		{
			"name": "Get HAML for lists",
			"identifier": "onRunHaml",
			"shortcut": "ctrl alt command h",
			"script": "markup-listify.js",
			"handler": "onRunHaml"
		}
	],
	"menu": {
		"title": "Markup Listify",
		"items": [
			"onRunMarkup",
			"onRunJade",
			"onRunMarkdown",
			"onRunHaml"
		]
	}
}